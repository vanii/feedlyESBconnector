<?xml version="1.0" encoding="UTF-8"?>
<proxy xmlns="http://ws.apache.org/ns/synapse"
       name="feedly_getFeedMetadata"
       transports="https,http"
       statistics="disable"
       trace="disable"
       startOnLoad="true">
    <target>
        <inSequence>
            <property name="feedId" expression="json-eval($.feedId)"/>
            <feedly.getFeedMetadata>
                <feedId>{$ctx:feedId}</feedId>
            </feedly.getFeedMetadata>
            <respond/>
        </inSequence>
        <outSequence>
            <log/>
            <send/>
        </outSequence>
    </target>
    <description/>
</proxy>
